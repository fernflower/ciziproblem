<script>
    function checkDateValidity(){
	if(jQuery('#uchazec-den').val() && jQuery('#uchazec-mesic').val() && jQuery('#uchazec-rok').val() && moment(jQuery('#uchazec-den').val()+"."+jQuery('#uchazec-mesic').val()+"."+jQuery('#uchazec-rok').val(), 'D.M.YYYY',true).isValid() === false){
	    alert('Neplatné datum narození!');
	   /* jQuery.alert({
		    title: 'Chyba',
		    content: 'Neplatné datum narození!',
		});
		*/
	}
    }
    
    function init(){
	jQuery('#uchazec-den').on('change',function(){ checkDateValidity();});
	jQuery('#uchazec-mesic').on('change',function(){ checkDateValidity();});
	jQuery('#uchazec-rok').on('change',function(){ checkDateValidity();});
	
	
	if(jQuery('#frm-tel_predvolba').val()=='420'){
	    jQuery('#frm-telefon').simpleMask({ 
		'mask': ['### ### ###'] 
		});
	}
	
      jQuery('#frm-tel_predvolba').on('change',function(){
	 if(jQuery('#frm-tel_predvolba').val()=='420'){
	    jQuery('#frm-telefon').simpleMask({ 
		'mask': ['### ### ###'] 
		});
	}
	else {
	jQuery('#frm-telefon').simpleMask('unmask');
	}
    });
      /*
      jQuery( "#trvalyPobytMesto" ).on('change',function(){
	   //jQuery('#trvalyPobytId').val('');
	   //jQuery('#trvalyPobyt').val('');
	    
      }).autocomplete({
	source: "https://127.0.0.1/trvaly-pobyt/a2/wp-content/themes/newspotrika/mistoPobytuSuggest.php",
	minLength: 2,
	select: function( event, ui ) {
	    jQuery('#trvalyPobytId').val(ui.item.id);
	    jQuery('#trvalyPobyt').val(ui.item.value);
	    
	    //alert("Selected: " + ui.item.value + " aka " + ui.item.id );
	}
	}); 
	*/
                
            jQuery('#zvlastni_potreby').on('change',function(){
                if(jQuery(this).val() == 'ano'){ jQuery('#zvlastni_potreby_detail').show();}
                else { jQuery('#zvlastni_potreby_detail').hide();}
              });
              
               jQuery('#forma_uhrady').on('change',function(){
                if(jQuery(this).val() == 'poukaz'){ jQuery('#cislo_poukazu').show();}
                else { jQuery('#cislo_poukazu').hide();}
              });
             
        }
        
 jQuery(document).ready(function(){
    init(); 
 });
 
</script>


